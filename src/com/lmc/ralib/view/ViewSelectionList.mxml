<?xml version="1.0" encoding="utf-8"?>
<components:ViewBackBase xmlns:fx="http://ns.adobe.com/mxml/2009" viewActivate="viewActivateHandler(event)" 
						 xmlns:s="library://ns.adobe.com/flex/spark" creationComplete="creationCompleteHandler(event)" 
						 xmlns:mx="library://ns.adobe.com/flex/mx" xmlns:components="com.lmc.ralib.components.*">
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	<fx:Script>
		<![CDATA[
			import mx.events.FlexEvent;
			
			import spark.events.ViewNavigatorEvent;
			
			protected function viewActivateHandler(event:ViewNavigatorEvent):void
			{
				list.dataProvider= data.listdata; 
			}
			
			protected function creationCompleteHandler(event:FlexEvent):void
			{
				title = data.title;
			}
			
			override public function createReturnObject():Object{
				return data;
				
			}
			
		]]>
	</fx:Script>
	<components:SearchList id="list" width="100%" height="100%" />
</components:ViewBackBase>
